  "private": true,
  "description": "",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "Sass",
    "framework": "React"
  },
  "scripts": {
    "eslint": "npx eslint -c eslint.config.js '**/*.{js,jsx,ts,tsx}' && npx tsc -p tsconfig.json --skipLibCheck",
    "stylelint": "npx stylelint --aei --config stylelint.config.js '**/*.{css,less,scss}'",
    "prettier": "npx prettier --write '**/*.{js,jsx,ts,tsx,md,html,css,less,scss}'",
    "iconfont": "npx rimraf src/components/icon && npx antm-icon",
    "prepare": "husky",
    "new": "taro new",
    "build:weapp": "taro build --type weapp",
    "dev:weapp": "npm run build:weapp -- --watch"
  },
  "browserslist": {
    "development": [
      "defaults and fully supports es6-module",
      "maintained node versions"
    ],
    "production": [
      "last 3 versions",
      "Android >= 4.1",
      "ios >= 8"
    ]
  },
